            if ($(window).width() > 768 && $(window).width() <= 992) {
                $(".reward").hide();
            }
        })
        $(window).resize(function () {
            if ($(window).width() <= 768) {
                $(".levelmap").hide();
                $(".reward").hide();
            }
            if ($(window).width() > 768 && $(window).width() <= 992) {
                $(".levelmap").show();
                $(".reward").hide();
