@model List<HaloMCC.Controllers.HomeController.XboxAchievements>
@{
    List<HaloMCC.Controllers.HomeController.XboxAchievements> halo1 = new List<HaloMCC.Controllers.HomeController.XboxAchievements>();
    List<HaloMCC.Controllers.HomeController.XboxAchievements> halo2 = new List<HaloMCC.Controllers.HomeController.XboxAchievements>();
    List<HaloMCC.Controllers.HomeController.XboxAchievements> halo3 = new List<HaloMCC.Controllers.HomeController.XboxAchievements>();
    List<HaloMCC.Controllers.HomeController.XboxAchievements> halo4 = new List<HaloMCC.Controllers.HomeController.XboxAchievements>();
    List<HaloMCC.Controllers.HomeController.XboxAchievements> universal = new List<HaloMCC.Controllers.HomeController.XboxAchievements>();
    foreach (HaloMCC.Controllers.HomeController.XboxAchievements cheevo in Model)
    {
        switch (cheevo.game)
        {
            case "1":
                halo1.Add(cheevo);
                break;
            case "2":
                halo2.Add(cheevo);
                break;
            case "3":
                halo3.Add(cheevo);
                break;
            case "4":
                halo4.Add(cheevo);
                break;
            case "5":
                universal.Add(cheevo);
                break;
        }
    }
}

<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
