            <th>Description</th>
            <th class="levelmap">Level/Map</th>
            <th>Unlocked</th>
            <th class="reward">Reward</th>
        </tr>
        @foreach (HaloMCC.Controllers.HomeController.XboxAchievements cheevo in halo1)
